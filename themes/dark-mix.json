      "settings": {
        "foreground": "#c5c5c5"
      }
    },
    {
      "scope": ["meta.jsx.children"],
      "settings": {
        "foreground": "#b8b8b8"
      }
    },
    {
      "scope": ["meta.template.expression", "source.ini"],
